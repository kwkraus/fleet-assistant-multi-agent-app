{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
  },
  "BlobStorage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "DefaultContainer": "fleet-documents",
    "MaxFileSizeBytes": 10485760,
    "AllowedExtensions": [".pdf", ".doc", ".docx", ".txt", ".jpg", ".jpeg", ".png", ".gif", ".xlsx", ".csv"]
  },
  "FileUpload": {
    "MaxFileSizeBytes": 3145728,
    "MaxFileCount": 2,
    "AllowedFileTypes": [".pdf", ".txt", ".docx", ".doc", ".jpg", ".jpeg", ".png", ".gif", ".bmp", ".csv", ".xlsx", ".xls"]
  },
  "UseFoundryAgent": true,
  "OpenAPIBaseUrl": "https://fleetassistant.azurewebsites.net",
  "FoundryAgentService": {
    "AgentId": "YOUR_AGENT_ID_HERE",
    "AgentEndpoint": "YOUR_FOUNDRY_AGENT_ENDPOINT_HERE",
    "RunPollingDelayMs": 100,
    "StreamingDelayMs": 50
  },
  "ApplicationInsights": {
    "ConnectionString": "YOUR_APPLICATION_INSIGHTS_CONNECTION_STRING"
  }
}
